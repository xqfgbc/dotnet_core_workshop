pipeline {
    agent any

    stages {
        stage('Build') {
            steps {
                echo 'Building...'
                powershell(returnStatus: true, script: '.\\buildAndPublish.ps1')
            }
        }
        stage('Test') {
            steps {
                echo 'Testing..'
            }
        }
        stage('Deploy') {
            steps {
                echo 'Deploying....'
            }
        }
    }
}